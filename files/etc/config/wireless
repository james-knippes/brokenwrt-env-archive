
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '36'
	option hwmode '11a'
	option path 'virtual/mac80211_hwsim/hwsim0'
	option htmode 'VHT80'
	option disabled '1'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option mode 'ap'
	option ssid 'OpenWrt'
	option encryption 'none'

config wifi-device 'radio1'
	option type 'mac80211'
	option channel '36'
	option hwmode '11a'
	option path 'virtual/mac80211_hwsim/hwsim1'
	option htmode 'VHT80'
	option disabled '1'

config wifi-iface 'default_radio1'
	option device 'radio1'
	option mode 'ap'
	option ssid 'OpenWrt'
	option encryption 'none'

config wifi-device 'radio2'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path 'platform/soc/a000000.wifi'
	option htmode 'HT20'
	option country 'DE'

config wifi-iface 'default_radio2'
	option device 'radio2'
	option mode 'ap'
	option encryption 'none'
	option ssid 'ch_000'
	option network 'ch_000'
	option macfilter 'allow'
	list maclist 'd4:5d:64:99:70:3e'
	option hidden '1'
	option disabled '1'

config wifi-device 'radio3'
	option type 'mac80211'
	option channel '36'
	option hwmode '11a'
	option path 'platform/soc/a800000.wifi'
	option htmode 'VHT80'
	option country 'DE'

config wifi-iface 'default_radio3'
	option device 'radio3'
	option mode 'ap'
	option network 'mgmt'
	option key 'a*O%=|>GX1H*s=!nhy@TB&>G/'
	option ssid 'bwrt_mgmt'
	option encryption 'psk2+ccmp'

config wifi-device 'radio4'
	option type 'mac80211'
	option channel '36'
	option hwmode '11a'
	option path 'platform/soc/8af8800.usb3/8a00000.dwc3/xhci-hcd.0.auto/usb1/1-1/1-1:1.0'
	option htmode 'VHT80'
	option disabled '1'

config wifi-iface 'default_radio4'
	option device 'radio4'
	option mode 'ap'
	option ssid 'OpenWrt'
	option encryption 'none'

config wifi-iface 'wifinet5'
	option network 'ch_001'
	option ssid 'ch_001'
	option encryption 'wep-open'
	option device 'radio2'
	option mode 'ap'
	option key1 's:HZyrF0tmwvEu3'
	option key '1'
	option disabled '1'

config wifi-iface 'wifinet6'
	option ssid 'ch_002'
	option encryption 'psk2+ccmp'
	option device 'radio2'
	option mode 'ap'
	option network 'ch_002'
	option key 'interpol'

