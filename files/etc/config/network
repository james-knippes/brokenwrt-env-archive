
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option ula_prefix 'fd66:de9e:e669::/48'

config interface 'lan'
	option ifname 'eth0'
	option proto 'static'
	option ipaddr '10.1.20.10'
	option netmask '255.255.255.0'
	option gateway '10.1.20.1'
	list dns '10.1.20.1'

